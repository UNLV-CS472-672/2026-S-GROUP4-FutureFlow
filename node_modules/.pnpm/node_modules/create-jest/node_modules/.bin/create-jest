#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Yoda/Desktop/FF_proj/2026-S-GROUP4-FutureFlow/node_modules/.pnpm/create-jest@29.7.0_@types+n_b42ce220410c8986b1ff3470be9c8d52/node_modules/create-jest/node_modules:/mnt/c/Users/Yoda/Desktop/FF_proj/2026-S-GROUP4-FutureFlow/node_modules/.pnpm/create-jest@29.7.0_@types+n_b42ce220410c8986b1ff3470be9c8d52/node_modules:/mnt/c/Users/Yoda/Desktop/FF_proj/2026-S-GROUP4-FutureFlow/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Yoda/Desktop/FF_proj/2026-S-GROUP4-FutureFlow/node_modules/.pnpm/create-jest@29.7.0_@types+n_b42ce220410c8986b1ff3470be9c8d52/node_modules/create-jest/node_modules:/mnt/c/Users/Yoda/Desktop/FF_proj/2026-S-GROUP4-FutureFlow/node_modules/.pnpm/create-jest@29.7.0_@types+n_b42ce220410c8986b1ff3470be9c8d52/node_modules:/mnt/c/Users/Yoda/Desktop/FF_proj/2026-S-GROUP4-FutureFlow/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/create-jest.js" "$@"
else
  exec node  "$basedir/../../bin/create-jest.js" "$@"
fi
