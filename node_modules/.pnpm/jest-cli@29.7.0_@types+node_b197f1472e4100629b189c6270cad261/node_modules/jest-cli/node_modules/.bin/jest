#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Yoda/Desktop/FF_proj/2026-S-GROUP4-FutureFlow/node_modules/.pnpm/jest-cli@29.7.0_@types+node_b197f1472e4100629b189c6270cad261/node_modules/jest-cli/node_modules:/mnt/c/Users/Yoda/Desktop/FF_proj/2026-S-GROUP4-FutureFlow/node_modules/.pnpm/jest-cli@29.7.0_@types+node_b197f1472e4100629b189c6270cad261/node_modules:/mnt/c/Users/Yoda/Desktop/FF_proj/2026-S-GROUP4-FutureFlow/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Yoda/Desktop/FF_proj/2026-S-GROUP4-FutureFlow/node_modules/.pnpm/jest-cli@29.7.0_@types+node_b197f1472e4100629b189c6270cad261/node_modules/jest-cli/node_modules:/mnt/c/Users/Yoda/Desktop/FF_proj/2026-S-GROUP4-FutureFlow/node_modules/.pnpm/jest-cli@29.7.0_@types+node_b197f1472e4100629b189c6270cad261/node_modules:/mnt/c/Users/Yoda/Desktop/FF_proj/2026-S-GROUP4-FutureFlow/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/jest.js" "$@"
else
  exec node  "$basedir/../../bin/jest.js" "$@"
fi
